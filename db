Create table _USER (
   _ID       INTEGER,  
   EMAIL     CHAR(100),   
   _PASSWORD  CHAR(100),   
   _POINT  INTEGER,   
   COLLEGE  CHAR(100),   
   PRIMARY KEY(_ID)
   );
CREATE TABLE POINTS (
   USER_ID   INTEGER,
   AMOUNT	 INTEGER,
   _TIMESTAMP  timestamp,
   TOTAL	  INTEGER
   );
   
CREATE TABLE COURSE (
   _ID 		  CHAR(100),
   DESCRIPTION CHAR(100)
);

CREATE TABLE STUDYNOTE (
   _ID 		   CHAR(100),
   UPLOADER_ID 	INTEGER,
   COURSE_ID	 CHAR(100),
   _FILE 		 CHAR(100),
   _TIMESTAMP  timestamp,
   PRIMARY KEY(_ID)
);

CREATE TABLE DOWNLOAD (
   _ID		 CHAR(100),
   USER_ID 	 INTEGER,
   STUDYNODE_ID  CHAR(100),
   PRIMARY KEY(_ID)
);

CREATE TABLE POST (
   _ID 		  CHAR(100),
   COURSE_ID  CHAR(100),
   USER_ID		integer,
   INFORMATION CHAR(250),
   TEIMSTAMP   TIMESTAMP,
   RATING 	   CHAR(100),
   MAIN_POST_ID CHAR(100)
);

CREATE TABLE RATING (
   POST_ID 		CHAR(100),
   USER_ID		INTEGER,
   COUNT_UP		INTEGER,
   COUNT_DOWN	INTEGER,
   primary KEY(POST_ID)
);
